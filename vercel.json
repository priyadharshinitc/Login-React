{
    "version": 2,
    "projects": {
      "frontend": {
        "root": "./frontend",
        "buildCommand": "npm run build",
        "outputDirectory": "build"
      },
      "backend": {
        "root": "./backend"
      }
    },
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/api/$1"
      }
    ]
}